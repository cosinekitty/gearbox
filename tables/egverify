#!/bin/bash
dotnet build -c Release -p ../src/BoardTest
for nonkings in Q r P QQ Qq QR Qr QB Qb QN Qn QP Qp RR Rr RB Rb RN Rn RP Rp BB BN BP Bp NP Np PP Pp; do
    echo ${nonkings}
done

